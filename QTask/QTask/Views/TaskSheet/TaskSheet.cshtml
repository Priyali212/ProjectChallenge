			<input type="text" class="f-input" id="txtRemarks" />
			</div> *@

			<div class="f-input-div filter-col">
				<label>Request From </label>
				<input type="text" class="f-input" id="txtReqForm" />
				@* <select class="f-input" name="ddbyname" id="txtReqForm">
				</select> *@
			</div>
			<div class="f-input-div filter-col">
				<label>Request Date </label>
				<input type="text" class="f-input" id="txtReqFormdt" placeholder="yyyy-mm-dd" />
			</div>
		</div>

		<div class="Est_div">
			<div class="add-new-form gap-1 mbt-1 grid grid-col-4 tab-grid-col-4 mob-grid-col-1">
				<div class="f-input-div filter-col">
					<label>Estimated Plan Date </label>
					<input type="text" class="f-input" id="txtEstPlanDt" placeholder="yyyy-mm-dd" />
				</div>
				<div class="f-input-div filter-col">
					<label>Estimated Completion Date </label>
					<input type="text" class="f-input" id="txtEstCompDt" placeholder="yyyy-mm-dd" />
				</div>
				<div class="f-input-div filter-col">
					<label>Estimated Hours</label>
					<input type="text" class="f-input" id="txtEsthrs" />
				</div>
				<div class="f-input-div filter-col">
					<label>Status</label>
					<select class="f-input" name="ddStatus" id="ddStatus">
						<option value="">Select Status</option>
						<option value="Yet To Start">Yet To Start</option>
						<option value="In Progress">In Progress</option>
						<option value="UAT">UAT</option>
						<option value="Live">Live</option>
						<option value="OnHold">OnHold</option>
						<option value="Completed">Completed</option>
					</select>
				</div>
			</div>
		</div>

		<div class="Add-div">
			<div class="add-new-form gap-1 mbt-1 grid grid-col-4 tab-grid-col-4 mob-grid-col-1">
				<div class="f-input-div filter-col">
					<label>Actual Start Date </label>
					<input type="text" class="f-input" id="txtActstartDt" placeholder="yyyy-mm-dd" />
				</div>
				<div class="f-input-div filter-col">
					<label>Actual End Date </label>
					<input type="text" class="f-input" id="txtActendDt" placeholder="yyyy-mm-dd" />
				</div>
				<div class="f-input-div filter-col">
					<label>Actual Hours </label>
					<input type="text" class="f-input" id="txtActhrs" />
				</div>
				<div class="f-input-div filter-col">
					<label>Assigned To</label>
					@* <input type="text" class="f-input" id="txtAssign" /> *@
					<select class="f-input" name="txtAssign" id="txtAssign">
					</select>
				</div>
			</div>

		</div>
		<div class="add-new-form gap-1 mbt-1 grid grid-col-1 tab-grid-col-1 mob-grid-col-1">


			<div class="f-input-div filter-col">
				<label>Description</label>
				<textarea rows="3" class="f-input" id="txtdescription"></textarea>
			</div>



		</div>

	</div>





	<div class="other-div">
		<div class="div-flex gap-1 justify-right pbt-1">
			@if (ViewBag.Edit != null && ViewBag.Edit)
			{
				<button class="button blue-btn" id="btnSave" Jira-Id="" Timesheet-id="" onclick="validatetasksheet('create')"><i class="fa fa-save"></i> Save</button>
			}
			<button class="button blue-btn" id="btnCancel"><i class="fa fa-times"></i> Cancel</button>

